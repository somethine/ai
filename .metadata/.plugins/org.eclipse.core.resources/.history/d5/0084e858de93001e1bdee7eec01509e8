package javaBasic.ch04.sec02;

public class 복습문제 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	
for( int k=50; k >=1; k--)	{
	System.out.println(k);

	for( int i=1 , j=10  ; i<=10 || j <= 19  ; i++, j++ )	{
		System.out.println( i +  + j);

}











}
	
	
	
	
	
	
	}

}
