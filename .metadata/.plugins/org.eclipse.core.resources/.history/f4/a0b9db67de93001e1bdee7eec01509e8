package javaBasic.ch04.sec02;

public class 복습문제 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	// for(초기값 조건식 증가값)
for( int k=50; k >=1; k--)	
	System.out.println(k);

for( int i=1 , j=10  ; i<=10 || j <= 19  ; i++, j++ )	{
		System.out.println( i +  + j);}

for( int a=1; a <=3; a++) {	
	System.out.println(a);
	for(int b=1 ; b<= b+1 ; b++) {
       System.out.print( a + "  ");
	}

        System.out.println(); 
	
	
	
	
	}

}
}